{
  "name": "MyNextApp",
  "version": "1.0.0",
  "description": "Base Project For Next",
  "main": "index.js",
  "keywords": [
    "learning",
    "nextjs",
    "reactjs"
  ],
  "author": "Afsane Fadaei",
  "license": "ISC",
  "scripts": {
    "dev": "node server.js",
    "build": "next build ./src",
    "start": "next start ./src",
    "cypress": "cypress open",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "yarn run --silent lint:js:fix",
      "git add"
    ],
    "src/**/*.scss": [
      "npm run --silent lint:scss:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@stomp/stompjs": "^5.4.2",
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.20.7",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.2",
    "cypress": "^3.4.1",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "graphql-hooks": "^4.0.0",
    "graphql-hooks-memcache": "^1.2.2",
    "graphql-hooks-ssr": "^1.1.3",
    "husky": "^3.0.8",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.4.2",
    "next": "^9.0.2",
    "next-cookies": "^1.1.3",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-d3-speedometer": "^0.7.0",
    "react-dom": "^16.9.0",
    "react-highlight-words": "^0.16.0",
    "react-syntax-highlighter": "^11.0.2",
    "react-use-websocket": "^0.9.4",
    "reactstrap": "^8.0.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.3.2",
    "websocket": "^1.0.29"
  },
  "devDependencies": {
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "less": "^3.10.2",
    "next-compose-plugins": "^2.2.0",
    "null-loader": "^3.0.0"
